public class CaseViceVersa {
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
		int n=0;
		Character c=sc.next().charAt(0);
		if(c>=65 &&c<=90) {
			n=(int)c+32;
			System.out.println((char)n);
		}
		else {
			n=(int)c-32;
			System.out.println((char)n);
		}
			
		

	}

}
